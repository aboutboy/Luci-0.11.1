require("luci.sys")
m = Map("todaair/todaair","Todaair","Todaair index page")

s = m:section(TypedSection, "todaair","")

name = s:option(Value, "sys", translate("Username"))
name2 = s:option(ListValue, "password", translate("Username"))
name2:value("static","Static")
name2:value("dhcp","Dhcp")
name2.default="dhcp"


return m
